{
  "name": "exportify",
  "version": "1.0.0",
  "description": "CLI tool to identify deep imports from internal packages in monorepos and generate package.json exports",
  "type": "module",
  "main": "dist/index.js",
  "bin": "dist/cli.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "monorepo",
    "deep-imports",
    "exports",
    "cli",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "glob": "^10.0.0"
  },
  "packageManager": "yarn@4.5.3"
}
